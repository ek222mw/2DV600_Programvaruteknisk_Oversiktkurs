package Exercise2;

import java.util.ArrayList;
import java.util.List;


public class Vehicle {

	private int countSpace = 0;
	private int countVehicles = 0;
	List vehiclelist = new ArrayList<>(); 
	
	public int getTotSpace()
	{
		return countSpace;
	}
	
	public void AddSpace(int n)
	{
		countSpace = countSpace +n;
	}
	
	public void AddVehicle(Object obj)
	{
		countVehicles++;
		vehiclelist.add(obj);
	}
	
	
}
