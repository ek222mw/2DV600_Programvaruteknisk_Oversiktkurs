package Deck;

public class Deck {
	
	
	private static Card[] cards;
	
	
	public Card shuffle(Card c)
	{
		
		if(!c.equals(51))
		{
			try {
				throw new Exception("not 52 cards in the deck.");
			} catch (Exception e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
		
		
		return c;
		
	}

	public static Card[] getCards() {
		
		cards = new Card[52];
		
		
		return cards;
		
		
		
	}

}
