
package Exercise5;

import Exercise5.TreeWordSet.BST;

//interface describing BST object stack

public interface IntStack extends Iterable<BST> {
	/* Add integer at top of stack. */
	public void push(BST n);
	
	/* Returns and removes integer at top of stack  */
	public BST pop();
	
	/* Returns without removing integer at top of stack */
	public BST peek();
	
	/* Number of integers currently stored. */
	public int size();
	
	/* Returns true if collection is empty. */
	public boolean isEmpty();
	
	/* String of type "[ 7 56 -45 68 ... ]" */
	public String toString();
}
